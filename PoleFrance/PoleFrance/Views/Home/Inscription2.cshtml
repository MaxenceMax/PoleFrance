                    @Html.EditorFor(model => model.Candidature.NumLicencie, new { htmlAttributes = new { @class = "feedback-input-little", @placeholder = "Numéro de licence" } })
                    @Html.EditorFor(model => model.Candidature.NumLicencie, new { htmlAttributes = new { @class = "feedback-input-little", @placeholder = "Numéro de licence" } })
