
@model PoleFrance.RouteConfig

@{
    ViewBag.Title = "Admin";
}
<div class="contenu">
    
    <div class="start">
        <div class="start-total">
            <div class="start-intermediaire">
                <a href="http://www.ffjudo.com/" id="logo" title="retour Accueil">
                    <img src="~/Content/Images/logo.png" alt="Fédération Française de Judo">
                </a>
                <div class="start-content">
                    <h1> Gestion Administrateur </h1>
                    <span>Bienvenue sur la page dédiée à la gestion de l'administrateur.</span>
                </div>
            </div>
        </div>
    </div>
    <div class="etape1">
        <div class="arrow-down-white"></div>

    </div>
    <div class="arrow-down-blue"></div>

    
    <div id="form-div-big">
        <div class="admin-block-conteneur-alignement">
            <div class="admin-block-conteneur">
                <div class="admin-block" onclick="location.href='@Url.Action("ListeResponsable", "Gestion")'">
                    <div class="admin-block-titre">Gestion Responsable</div>
                    <div class="admin-block-image"><img src="@Url.Content("~/Content/Images/Admin/resp.png")" /></div>
                    <div class="admin-block-desc">Dans cette section vous pouvez ajouter, modifier et supprimer des responsables</div>
                </div>
                <div class="admin-block" onclick="location.href=@Url.Action("OuvertureInscription", "Gestion")">
                    <div class="admin-block-titre">Etat des inscriptions</div>
                    <div class="admin-block-image"><img src="@Url.Content(ViewBag.image)" /></div>
                    <div class="admin-block-desc">@ViewBag.texte</div>
                </div>
            </div>
        </div>

        

    </div>









    </div>